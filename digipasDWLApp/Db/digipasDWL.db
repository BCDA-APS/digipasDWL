record(ai, "$(P)$(R):X") {
    field(DTYP, "asynFloat64")
    field(INP, "@asyn($(PORT),$(ADDR=0))DUAL_X_DEG")
    field(EGU, "deg")
    field(PREC, 3)
    field(SCAN, "I/O Intr")
}

